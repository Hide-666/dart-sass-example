{
  "name": "dart-sass-example",
  "version": "1.0.0",
  "main": "scss/style.scss",
  "scripts": {
    "start": "npm run sass:watch",
    "sass:watch": "chokidar 'scss/**/*.scss' -c 'npm run sass:build' --initial",
    "sass:build": "sass scss/style.scss public/style.css --no-source-map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kojika17/dart-sass-example.git"
  },
  "devDependencies": {
    "chokidar-cli": "^2.1.0",
    "sass": "^1.26.5"
  }
}
